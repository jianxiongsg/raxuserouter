{
    "presets": [
        [
            "@babel/preset-env",
            {
                "debug":true,
                "useBuiltIns":false,
                "targets":[
                    "> 1%",
                    "last 2 versions",
                    "not ie <= 8"
                ]
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime"
        ],
        ["@babel/plugin-transform-react-jsx"]
    ]
}